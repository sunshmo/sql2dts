{
  "name": "sql2dts",
  "version": "0.0.1",
  "description": "A command-line tool that converts SQL files into TypeScript type definition (.d.ts) files that support multiple database types. Can also be used in the browser.",
  "main": "dist/index.js",
  "bin": {
    "sql2dts": "./bin/index.js"
  },
  "scripts": {
    "generate-export": "ts-node generate-export.ts",
    "tsup": "tsup",
    "dev": "ts-node bin/index.ts test/mysql.sql -d mysql",
    "build": "run-s format generate-export tsup",
    "format-view": "biome check src",
    "format": "biome format --write src",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sql2dts",
    "sql",
    "dts",
    "generate",
    ".d.ts",
    "typescript"
  ],
  "author": "sunshmo",
  "license": "MIT",
  "packageManager": "pnpm@10.7.0",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.14.1",
    "npm-run-all": "^4.1.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0"
  }
}
